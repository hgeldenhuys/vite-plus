{
  "name": "vite-react-plus",
  "version": "0.1.12",
  "description": "Hooks and Components for making Valtio easier to work with React",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "**"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=12.20.0"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "bracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 80
  },
  "keywords": [
    "react",
    "state",
    "valtio",
    "proxy",
    "store"
  ],
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc --project .",
    "git:commit": "npm --no-git-tag-version version patch && git add . && git commit -m \"$(date)\" && git push"
  },
  "author": "Herman Geldenhuys",
  "homepage": "https://github.com/hgeldenhuys/vite-plus#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hgeldenhuys/vite-plus.git"
  },
  "bugs": {
    "url": "https://github.com/hgeldenhuys/vite-plus/issues"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8",
    "valtio": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "type": "module"
}
