{
  "name": "vite-react-plus",
  "version": "0.1.17",
  "description": "Hooks and Components for making Valtio easier to work with React",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "**"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=14"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "bracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 80
  },
  "keywords": [
    "react",
    "state",
    "valtio",
    "proxy",
    "store"
  ],
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc --project tsconfig.json --module ESNext --outDir ./dist",
    "git:commit": "npm --no-git-tag-version version patch && git add . && git commit -m \"$(date)\" && git push"
  },
  "author": "Herman Geldenhuys",
  "homepage": "https://github.com/hgeldenhuys/vite-plus#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hgeldenhuys/vite-plus.git"
  },
  "bugs": {
    "url": "https://github.com/hgeldenhuys/vite-plus/issues"
  },
  "license": "MIT",
  "dependencies": {
    "valtio": "^1.10.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "valtio": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "eslint": "^8.12.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
